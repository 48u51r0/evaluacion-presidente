weighted.sd <- function(x, w, na.rm = FALSE) {
if (na.rm) {
w <- w[i <- !is.na(x)]
x <- x[i]
}
var.w <- (sum(w*x^2) * sum(w) - sum(w*x)^2) / (sum(w)^2 - sum(w^2))
sqrt(var.w)
}
